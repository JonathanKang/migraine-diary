<interface domain="migraine-diary">
    <template class="MdWindow" parent="GtkApplicationWindow">
        <property name="height-request">800</property>
        <property name="width-request">640</property>
        <child type="titlebar">
            <object class="GtkHeaderBar">
                <property name="show-close-button">True</property>
                <property name="title">Migraine Diary</property>
                <property name="visible">True</property>
                <style>
                    <class name="titlebar"/>
                    <class name="event_toolbar"/>
                </style>
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <property name="visible">True</property>
                <child>
                    <object class="MdDiaryList" id="diary_list">
                        <property name="visible">True</property>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
