<interface domain="migraine-diary">
    <template class="MdWindow" parent="GtkApplicationWindow">
        <property name="height-request">800</property>
        <property name="width-request">640</property>
        <child type="titlebar">
            <object class="MdDiaryHeaderbar" id="headerbar">
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="orientation">vertical</property>
                <property name="visible">True</property>
                <child>
                    <object class="GtkStack" id="main_stack">
                        <property name="expand">True</property>
                        <property name="visible">True</property>
                        <child>
                            <object class="GtkLabel">
                                <property name="halign">GTK_ALIGN_CENTER</property>
                                <property name="valign">GTK_ALIGN_CENTER</property>
                                <property name="label">No diary found.</property>
                                <property name="visible">True</property>
                            </object>
                        </child>
                        <child>
                            <object class="MdDiaryList" id="diary_list">
                                <property name="visible">True</property>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>
